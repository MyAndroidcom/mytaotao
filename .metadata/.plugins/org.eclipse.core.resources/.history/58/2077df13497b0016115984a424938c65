</head>
<body>
	<script type="text/javascript">
		alert($);
		$(function(){
			$.ajax({
				type:"GET",
				url:"http://localhost/json.jsp";
				dataType:"jsonp";
				success:function(data){
				alert(data.abc);
			}
			});
		});
	</script>
</body>
